{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "indent": ["error", 4],
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-unused-vars": "warn"
    }
  }
  